{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "prepare": "ts-patch install && typia patch",
    "dev:front": "vite",
    "dev:back": "rollup -c --watch --watch.onEnd='pnpm run start:back'",
    "clean": "rm -vrf dist/",
    "clean:front": "rm -vrf dist/front/",
    "clean:back": "rm -vrf dist/back/",
    "build": "npm run build:front && npm run build:back",
    "build:front": "vite build",
    "build:back": "rollup -c",
    "start:front": "vite preview",
    "start:back": "node ./dist/back/multi-entry.js",
    "debug:back": "node --inspect-brk ./dist/back/multi-entry.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@types/node": "^20.11.24",
    "rollup": "^4.9.5",
    "rollup-plugin-typescript2": "^0.36.0",
    "solid-devtools": "^0.29.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "uvu": "^0.5.6",
    "vite": "^5.0.11",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-solid": "^2.8.2"
  },
  "dependencies": {
    "hyper-express": "^6.14.12",
    "solid-js": "^1.8.11",
    "magic-string": "^0.30.8",
    "typescript": "^5.3.3",
    "typia": "^5.5.0"
  }
}
